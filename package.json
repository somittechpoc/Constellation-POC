    "@pega/cosmos-react-work": "^7.1.2",
    "@pega/eslint-config": "^0.10.2",
    "@pega/tsconfig": "^0.15.1",
    "babel-loader": "^9.1.3",
    "dayjs": "^1.11.11",
    "fast-deep-equal": "3.1.3",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "npm": "^10.8.1",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-is": "^18.3.1",
    "react-redux": "^7.2.9",
    "serve": "^14.2.3",
    "shelljs": "^0.8.5",
    "styled-components": "^5.3.11",
    "ts-loader": "^9.5.1"
  },
  "devDependencies": {
    "@pega/configs": "^0.15.1",
